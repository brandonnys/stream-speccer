{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "submit.js",
      "use": "@vercel/node"
    }
  ],
"rewrites": [
  {
    "source": "/submit",
    "destination": "/submit.js"
  },
  {
    "source": "/(.*)",
    "destination": "/public/$1"
  }
],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
