{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "submit.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/submit",
      "dest": "/submit.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
