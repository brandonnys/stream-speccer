{
  "version": 2,
  "builds": [
    {
      "src": "api/submit.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/submit",
      "destination": "/api/submit"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}

